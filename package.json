{
  "name": "typescript-ai-agent",
  "version": "1.0.0",
  "description": "Production-ready AI agent built with TypeScript, converted from Kotlin AI Agent with Koog framework",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsx src/index.ts",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "keywords": [
    "ai",
    "agent",
    "openai",
    "mcp",
    "typescript",
    "automation"
  ],
  "author": "",
  "license": "Apache-2.0",
  "dependencies": {
    "openai": "^4.77.3",
    "axios": "^1.7.9",
    "dotenv": "^16.4.7",
    "chalk": "^5.4.1",
    "inquirer": "^12.3.0",
    "diff": "^7.0.0",
    "zod": "^3.24.1",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/sdk-node": "^0.53.0",
    "@opentelemetry/exporter-trace-otlp-http": "^0.53.0",
    "@opentelemetry/semantic-conventions": "^1.27.0"
  },
  "devDependencies": {
    "@types/node": "^22.10.7",
    "@types/inquirer": "^9.0.7",
    "@types/diff": "^5.2.3",
    "typescript": "^5.7.3",
    "tsx": "^4.19.2",
    "vitest": "^2.1.8",
    "@vitest/coverage-v8": "^2.1.8",
    "eslint": "^9.18.0",
    "@typescript-eslint/parser": "^8.19.1",
    "@typescript-eslint/eslint-plugin": "^8.19.1",
    "prettier": "^3.4.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
